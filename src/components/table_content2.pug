mixin table2()
    div(class="container-xl")
        div(class="table-responsive")
            div(class="table-wrapper")
                div(class="table-title")
                    div(class="row")
                        div(class="col-sm-6")
                            h2 Manage 
                                b Residents
                                //TODO: decide if i want add/delete for certain roles on this page
                        div(class="col-sm-6")
                            a(href="#addResidentModal" class="btn btn-success" data-toggle="modal")
                                i(class="material-icons") &#xE147;
                                span Add New Resident
                            a(href="#deleteResidentModal" class="btn btn-danger" data-toggle="modal")
                                i(class="material-icons") &#xE15c;
                                span Delete Resident
                table(class="table table-striped table-hover")
                    thead
                        tr(class="text-center")
                            th
                                span(class="custom-checkbox")
                                    input(type="checkbox" id="selectAll")
                                    label(for="selectAll")
                                th ID
                                th Name
                                th Address
                                th Action
                    tbody
                        each boatRampData in data
                            tr(class="text-center")
                                td
                                    span(class="custom-checkbox")
                                    input(type="checkbox" id="checkbox1" name="options[]" value="1")
                                    label(for="checkbox1")
                                td= boatRampData.ID
                                td= boatRampData.First_Name + " " + boatRampData.Last_Name
                                td= boatRampData.House_Num + " " + boatRampData.Street
                                td
                                    a(href="#editResidentModal" class="edit" data-toggle="modal")
                                        i(class="material-icons" data-toggle="tooltip" title="Edit") &#xE254;
                                    a(href="#deleteResidentModal" class="delete" data-toggle="modal")
                                        i(class="material-icons" data-toggle="tooltip" title="Delete") &#xE872;
//TODO: Implement pagination in later versions.